var xing = xing || {};xing.bikinibottom = xing.bikinibottom || {};xing.bikinibottom.utils = {	convertToTwoDigitFormat: function(duration) {		var minutes, seconds, centiseconds, timeArray, timeArrayOutput, i, str;				minutes = Math.floor(duration / 60);		seconds = Math.floor(duration % 60);		centiseconds = Math.floor((duration - Math.floor(duration)) * 100);				timeArray = new Array(minutes, seconds, centiseconds);		timeArrayOutput = new Array();				for (i=0; i<timeArray.length; i++) {			str = String(timeArray[i]);			timeArrayOutput.push(				str.length < 2 ? ("0" + str) : str.substr(0, 2)			);		}				return timeArrayOutput.join(":");	},		convertToTimerFormat: function(duration) {		var seconds = Math.floor(duration % 60);		var minutes = Math.floor(duration / 60);				seconds = String(seconds);		var secondsStr = seconds.length < 2 ? ("0" + seconds) : seconds.substr(0, 2);				return minutes + ":" + secondsStr;	}};